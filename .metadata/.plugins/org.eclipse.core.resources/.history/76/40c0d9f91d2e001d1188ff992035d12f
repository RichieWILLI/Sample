package complex_Logical_Program;

public class FindTheSumOfEachDigit {

	public static void main(String[] args) {
		
		int a=123;
		int sum=0;
		int n=0;
		
		for(int i=0;i<=0;i++)
		{
			 n=a/100;
			sum=sum+n;
			
			a=a%10;
			if(a==0)
				break;
			
					
		}
		System.out.println(sum);
		
		
		
	}

}
