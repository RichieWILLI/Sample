package practice;

public class AscendingOrderProgramUsingBubbleSort {

	public static void main(String[] args) {
		
		int ary[]= {12,45,11,2,1};
		
		for(int i=0;i<ary.length;i++) {
			for(int j=i+1;j<ary.length;j++) {
				if(ary[j-1]>ary[j]) {
					int temp=ary[j-1];
					ary[j-1]=ary[j];
					ary[j]=temp;
				}
			}
			System.out.println(ary[i]);
		}
		
		
	}

}
