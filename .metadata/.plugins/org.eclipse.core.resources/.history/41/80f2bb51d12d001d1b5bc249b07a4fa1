package complex_Logical_Program;

public class ShiftingNumber {

	public static void main(String[] args) {
		
		//input is a[]={1,2,3,4,5} o/p= 3 4 5 1 2
		
		int a[]= {1,2,3,4,5};
		
		
		for(int i=0;i<a.length;i++) {
			
			int key=1;
			for(int k=0;k<key;k++) {
				int temp=a[i];
				for(int j=1;j<a.length;j++) {
					a[j-1]=a[j];
				}
				a[a.length-1]=temp;
				
			}
			
			System.out.print(a[i]);
		}
		
		
	}

}
